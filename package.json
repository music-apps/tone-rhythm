{
  "name": "tone-rhythm",
  "version": "1.0.0",
  "description": "Generate an array of Tone.Transport times for use in Tone.Part",
  "main": "dist/tone-rhythm.min.js",
  "scripts": {
    "test": "nyc mocha --exit './test/*.test.js'",
    "test-browser": "npm run build && open index.html # RUNS IN BROWSER WITH MOCHA",
    "dev": "webpack --watch --mode development",
    "build": "webpack -p",
    "doc": "doxdox src/tone-rhythm.js --layout markdown --output DOCUMENTATION.md"
  },
  "keywords": [
    "Javascript",
    "Music Composition",
    "Web Audio",
    "Web Audio API",
    "Playback",
    "Instrument",
    "Music",
    "Tone",
    "tone-rhythm"
  ],
  "author": "https://github.com/scraggo",
  "repository": {
    "type": "git",
    "url": "https://github.com/scraggo/tone-rhythm.git"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-loader": "^8.0.4",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "doxdox": "^3.0.0",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "sinon": "^7.3.1"
  },
  "peerDependencies": {
    "tone": ">=0.12.0"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "public",
      "**/test/**",
      "**.js",
      "testSetup"
    ],
    "reporter": [
      "text",
      "html"
    ]
  }
}
